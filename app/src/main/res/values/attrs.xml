<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="TitleEditext">
        <attr name="TitleEditext_title" format="string|reference" />
        <attr name="hint" format="string|reference" />
        <attr name="titletext_right_icon" format="reference" />
        <attr name="right_show" format="boolean" />

        <!--是否密码-->
        <attr name="ispassword" format="boolean"/>

    </declare-styleable>



    <declare-styleable name="TitleBarLayout">
        <attr name="titleText" format="string" />
        <attr name="showLeftBackIcon" format="boolean" />
        <attr name="showRightButton" format="boolean" />
        <attr name="showRightText" format="boolean" />
        <attr name="showRightImage" format="boolean" />
        <attr name="showFilterIcon" format="boolean" />
        <attr name="barLeftIcon" format="reference"/>
        <attr name="rightText" format="string" />
        <attr name="rightButtonText" format="string" />
        <attr name="showRightMenu2" format="boolean"/>
        <attr name="rightmenu2Icon" format="reference"/>
        <attr name="left_showlogo" format="boolean"/>
        <attr name="right_icon" format="reference"/>

    </declare-styleable>


    <declare-styleable name="ChargeGifView">
        <!--刻度的角度 -->
        <attr name="charge_angle" format="integer" />
        <!--圆环颜色-->
        <attr name="charge_circle_color" format="integer|reference" />
        <!--圆环的厚度-->
        <attr name="charge_circle_width" format="integer|reference" />
        <!--刻度的颜色-->
        <attr name="charge_scales_color" format="integer|reference" />
        <!--刻度的厚度-->
        <attr name="charge_scales_width" format="integer|reference" />
        <!--图标-->
        <attr name="charge_bitmap" format="reference"/>
    </declare-styleable>


    <declare-styleable name="DataItemView">
        <attr name="leftIcon" format="reference" />
        <attr name="showLeftIcon" format="boolean" />
        <attr name="showRightIcon" format="boolean" />
        <attr name="showSubName" format="boolean" />
        <attr name="showRedPoint" format="boolean" />
        <attr name="textSubNameColor" format="reference|color" />
        <attr name="itemName" format="string" />
        <attr name="subName" format="string" />
    </declare-styleable>



    <declare-styleable name="SettingView">
        <attr name="title" format="string|reference" />

    </declare-styleable>




    <declare-styleable name="Tab">
        <attr name="tabText" format="string" />
    </declare-styleable>

</resources>